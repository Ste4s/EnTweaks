{
  "name": "Evernote Tweaker",
  "version": "1.0.0",
  "manifest_version": 3,
  "description": "Makes minor tweaks to the Evernote web client to make it more usable.",
  "permissions": [],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "64": "icons/icon64.png",
    "128":"icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*.evernote.com/client/web*"],
      "js": ["contentScript.js"],
      "run_at": "document_end"
    }
  ]
}